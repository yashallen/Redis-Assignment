{
  "name": "website-node-react",
  "version": "1.0.0",
  "description": "Reactified Zefo Website",
  "repository": {
    "type": "git",
    "url": "git@github.com:gozefo/website-node-react.git"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3"
  },
  "author": "gozefo@github",
  "scripts": {
    "webpack:dev": "webpack --config ./internals/webpack/webpack.server.babel.js",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean && npm run test",
    "build": "npm run build:production",
    "build:production": "yarn && cross-env NODE_ENV=production NODE_WEBPACK_ENV=production UPLOAD_S3=1 webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "sourcemap:production": "yarn && cross-env NODE_ENV=production NODE_WEBPACK_ENV=production SOURCE_MAP_GEN=1 webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:staging": "yarn && cross-env NODE_ENV=production NODE_WEBPACK_ENV=staging UPLOAD_S3=1 webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:docker": "yarn && cross-env NODE_ENV=production NODE_WEBPACK_ENV=staging TEST_BUILD=1 webpack --config internals/webpack/webpack.docker.babel.js --color --profile --progress",
    "sourcemap:staging": "cross-env NODE_ENV=production NODE_WEBPACK_ENV=staging SOURCE_MAP_GEN=1 webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build:cp:staging": "./script/s3_build_cp.sh staging",
    "build:cp:production": "./script/s3_build_cp.sh production",
    "start": "cross-env NODE_ENV=development NODE_WEBPACK_ENV=development NODE_PATH=.:./server node $NODE_DEBUG_OPTION server",
    "wishListMigration": "cross-env NODE_ENV=production NODE_PATH=.:./server node $NODE_DEBUG_OPTION script/migrateWishList2.js",
    "start:debug": "cross-env NODE_ENV=development NODE_WEBPACK_ENV=development NODE_PATH=.:./server node debug server",
    "start:tunnel": "cross-env NODE_ENV=development NODE_WEBPACK_ENV=development ENABLE_TUNNEL=true NODE_PATH=.:./server node server",
    "start:staging": "npm run build:staging && npm run start:stage",
    "start:stage": "cross-env NODE_ENV=staging NODE_PATH=.:./server node server",
    "start:docker": "cross-env NODE_ENV=staging TEST_BUILD=1 NODE_PATH=.:./server node server",
    "start:production": "npm run build:production && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production NODE_PATH=.:./server node server",
    "start:configListenerTest": "cross-env NODE_ENV=development NODE_PATH=.:./server node $NODE_DEBUG_OPTION server/configBuilder",
    "start:configListener": "cross-env NODE_ENV=production NODE_PATH=.:./server node $NODE_DEBUG_OPTION server/configBuilder",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "generate:mobile": "NODE_PLOP_DEVICE=mobile plop --plopfile internals/generators/index.js",
    "generate:desktop": "NODE_PLOP_DEVICE=desktop plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:e2e:init": "node ./e2e/init.js",
    "test:e2e:desktop": "APP_DEVICE='desktop' nightwatch --config nightwatch.js",
    "test:e2e:mobile": "APP_DEVICE='mobile' nightwatch --config nightwatch.js --env chromeMobile",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "test:server": "mocha-parallel-tests server/**/*.test.js",
    "test:jest:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:jest:mobile": "cross-env NODE_ENV=test jest --config internals/testing/jest-mobile.config.js",
    "test:jest:mobile:watchAll": "cross-env NODE_ENV=test jest --config internals/testing/jest-mobile.config.js --watchAll",
    "test:jest:desktop": "cross-env NODE_ENV=test jest --config internals/testing/jest-desktop.config.js",
    "test:jest:desktop:watchAll": "cross-env NODE_ENV=test jest --config internals/testing/jest-desktop.config.js --watchAll",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "storybook:desktop": "start-storybook -p 6006 -c ./.storybook/desktop",
    "storybook:mobile": "STORYBOOK_APP=mobile start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:staging": "yarn && npm run build:staging && npm run deploy:staging:build:webhook && pm2 deploy ecosystem.config.js staging",
    "deploy:production": "yarn && npm run build:production && npm run deploy:production:build:webhook && pm2 deploy ecosystem.config.js prod",
    "deploy:staging:server": "pm2 deploy ecosystem.config.js staging",
    "deploy:production:server": "pm2 deploy ecosystem.config.js prod",
    "deploy:staging:build:webhook": "./script/deployment_webhook.sh website build test",
    "deploy:staging:server:webhook": "./script/deployment_webhook.sh website server test",
    "deploy:production:build:webhook": "./script/deployment_webhook.sh website build production",
    "deploy:production:server:webhook": "./script/deployment_webhook.sh website server production"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "plugins": [
      "styled-components"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul",
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true,
      "jest": true
    },
    "plugins": [
      "mocha",
      "redux-saga",
      "react",
      "jsx-a11y",
      "json"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "mocha/no-exclusive-tests": "error",
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.test.babel.js"
        }
      }
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "socket.io",
      "socket.io-emitter",
      "socket.io-redis",
      "fast-csv",
      "node-statsd",
      "sqs-consumer",
      "pm2-logrotate",
      "morgan",
      "cache-require-paths",
      "pm2",
      "bunyan",
      "razorpay",
      "newrelic",
      "passport",
      "passport-google-oauth",
      "passport-facebook",
      "moment",
      "querystring",
      "redis",
      "module",
      "webpack-s3-plugin",
      "cookies",
      "generic-pool",
      "chai-as-promised",
      "rewire",
      "mocha-parallel-tests",
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "agentkeepalive": "^3.3.0",
    "aws-sdk": "2.41.0",

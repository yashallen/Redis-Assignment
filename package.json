    "node-statsd": "^0.1.1",
    "nuka-carousel": "^2.2.1",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth": "1.0.0",
    "pm2": "2.2.3",
    "pm2-logrotate": "^2.2.0",
    "polished": "^1.1.2",
    "qs": "^6.3.0",
    "querystring": "^0.2.0",
    "raven-js": "^3.12.0",
    "razorpay": "^1.2.1",
    "rc-slider": "^5.4.3",
    "react": "15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "15.6.2",
    "react-draggable": "^2.2.3",
    "react-helmet": "3.1.0",
    "react-idle-timer": "^2.0.0",
    "react-image-magnify": "^2.0.4",
    "react-intl": "2.1.5",
    "react-lazyload": "^2.2.0",
    "react-metrics": "^2.2.3",
    "react-modal": "^2.2.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.3.3",
    "react-share": "^2.0.0",
    "react-swipeable": "3.7.0",
    "react-tabs": "^1.1.0",
    "react-test-renderer": "15.6.2",
    "react-tippy": "^1.2.1",
    "react-youtube": "^7.3.0",
    "redis": "^2.6.5",
    "redux": "3.6.0",
    "redux-form": "^6.5.0",
    "redux-immutable": "3.0.8",
    "redux-saga": "0.12.0",
    "reselect": "2.5.4",
    "response-time": "^2.3.2",
    "sanitize.css": "4.1.0",
    "socket.io": "^2.0.4",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "sqs-consumer": "3.5.0",
    "styled-components": "2.0.0-1",
    "superagent": "3.1.0",
    "uuid": "3.0.1",
    "warning": "3.0.0",
    "webpack-s3-plugin": "^0.9.2",
    "whatwg-fetch": "1.0.0",
    "xml-js": "^1.5.2"
  },
